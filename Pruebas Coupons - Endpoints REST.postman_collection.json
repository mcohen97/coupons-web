	"item": [
		{
			"name": "Evaluate promotion (discount) with correct data and applies",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "b3d35d8a-65ba-4e80-aebf-4fc1b79333da",
						"exec": [
							"pm.test(\"Status code is 200\",function(){",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var jsonData = pm.response.json();",
							"",
							"",
							"pm.test(\"Returns applicable, with correct return type and value\", function(){",
							"    pm.expect(jsonData).to.have.property('applicable');",
							"    pm.expect(jsonData).to.have.property('return_type');",
							"    pm.expect(jsonData).to.have.property('return_value');",
							"",
							"",
							"    pm.expect(jsonData.applicable).to.eql(true);",
							"    pm.expect(jsonData.return_type).to.eql(\"percentaje\");",
							"    pm.expect(jsonData.return_value).to.eql(10);",
							"",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "Authorization",
						"value": "{{organization1AppKey}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n\t\"code\": \"discount1\",\n\t\"attributes\": {\"total\": 15, \"quantity\": 3, \"transaction_id\":6}\n}"
				},
				"url": {
					"raw": "{{domain}}/promotions/evaluate",
					"host": [
						"{{domain}}"
					],
					"path": [
						"promotions",
						"evaluate"
					]
				}
			},
			"response": []
		},
