							"pm.test(\"Returns applicable, with correct return type and value\", function(){",
							"    pm.expect(jsonData).to.have.property('total_money_spent');",
							"    pm.expect(jsonData).to.have.property('invocations_count');",
							"    pm.expect(jsonData).to.have.property('positive_ratio');",
							"    pm.expect(jsonData).to.have.property('negative_ratio');",
							"",
							"",
							"    pm.expect(jsonData.total_money_spent).to.eql(101.5);",
							"    pm.expect(jsonData.invocations_count).to.eql(7);",
							"    pm.expect(jsonData.positive_ratio).to.eql(2/7);",
							"    pm.expect(jsonData.negative_ratio).to.eql(5/7);",
							"",
							"});",
