  </head>
  
</head>

<body class="rails-default-error-page">
  <!-- This file lives in public/404.html -->
  <div id="notfound">
		<div class="notfound-bg">
			<div></div>
			<div></div>
			<div></div>
			<div></div>
		</div>
		<div class="notfound">
			<div class="notfound-404">
				<h1>422</h1>
      </div>
      <h2>Unacceptable</h2>
			<img style="display: block; clear: both; margin: 0px auto 20px auto" src=<%= image_path('unacceptable.gif')%> height="200px">
